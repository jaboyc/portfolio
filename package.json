{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "docker compose --env-file .env.development up -d && next dev",
    "lint": "next lint",
    "build": "next build",
    "start": "next start",
    "dev:prisma:generate": "dotenv -e .env.development -- npx prisma generate",
    "dev:prisma:migrate": "dotenv -e .env.development -- npx prisma migrate dev",
    "dev:prisma:migrateCreateOnly": "dotenv -e .env.development -- npx prisma migrate dev --create-only",
    "dev:prisma:studio": "dotenv -e .env.development -- npx prisma studio",
    "dev:prisma:seed": "dotenv -e .env.development -- tsx scripts/seed.ts",
    "dev:db:dump": "docker exec -it postgres pg_dump -U root -d postgres --clean > data_dump.sql",
    "dev:db:restore": "docker exec -i postgres psql -U root -d postgres < data_dump.sql",
    "dev:db:restoreProd": "docker exec -i postgres psql -U root -d postgres < prod_data_dump.sql",
    "prod:prisma:resolve": "export $(cat .env.production | xargs) && npx prisma migrate resolve --applied ",
    "prod:prisma:generate": "export $(cat .env.production | xargs) && npx prisma generate",
    "prod:prisma:migrate": "dotenv -e .env.production -- npx prisma migrate deploy",
    "prod:prisma:studio": "dotenv -e .env.production -- npx prisma studio",
    "prod:prisma:seed": "dotenv -e .env.production -- tsx scripts/seed.ts",
    "prod:db:dump": "export $(cat .env.production | xargs) && pg_dump ${DATABASE_URL} --clean > prod_data_dump.sql",
    "prod:db:restore": "export $(cat .env.production | xargs) && psql ${DATABASE_URL} < data_dump.sql",
    "prod:db:restoreProd": "export $(cat .env.production | xargs) && psql ${DATABASE_URL} < prod_data_dump.sql"
  },
  "dependencies": {
    "@adminjs/cli": "^1.0.4",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@nextui-org/react": "^2.2.9",
    "@premieroctet/next-admin": "^3.0.1",
    "@prisma/client": "^5.7.1",
    "@vercel/analytics": "^1.1.1",
    "dotenv-cli": "^7.3.0",
    "framer-motion": "^10.16.16",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "next": "14.0.4",
    "prisma-json-schema-generator": "^3.1.5",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.33.1",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
